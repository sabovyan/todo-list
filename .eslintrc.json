{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true,
        "node": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": 12
    },
    "rules": {
        "linebreak-style": 0,
        "implicit-arrow-linebreak": 0,
        "import/extensions": 0,
        "import/prefer-default-export": 0,
        "class-methods-use-this": 0,
        "no-confusing-arrow": 0,
        "comma-dangle": 0,
        "function-paren-newline": 0
    }
}
