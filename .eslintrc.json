{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true,
        "node": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": 12
    },
    "rules": {
        "linebreak-style": 0,
        "implicit-arrow-linebreak": 0,
        "import/extensions": 0,
        "import/prefer-default-export": 0
    }
}
